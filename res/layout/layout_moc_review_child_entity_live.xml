<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:orientation="horizontal"
        android:id="@id/ll_moc_review_child_entity_background"
        android:background="#fffff0e0"
        android:padding="4.0dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <View
            android:layout_gravity="center"
            android:layout_width="18.0dip"
            android:layout_height="18.0dip" />
        <TextView
            android:textSize="14.0sp"
            android:textColor="#ff000000"
            android:id="@id/tv_moc_review_parameter"
            android:padding="4.0dip"
            android:layout_width="150.0dip"
            android:layout_height="wrap_content"
            android:text=""
            android:layout_weight="3.0" />
        <TextView
            android:textSize="12.0sp"
            android:textColor="#ff000000"
            android:gravity="right"
            android:id="@id/tv_moc_review_target"
            android:padding="4.0dip"
            android:layout_width="60.0dip"
            android:layout_height="wrap_content"
            android:text="Tgt"
            android:layout_weight="1.0" />
        <TextView
            android:textSize="14.0sp"
            android:textColor="#ff000000"
            android:gravity="right"
            android:id="@id/tv_moc_review_achievement"
            android:padding="4.0dip"
            android:layout_width="60.0dip"
            android:layout_height="wrap_content"
            android:text="Achv"
            android:layout_weight="1.0" />
        <TextView
            android:textSize="14.0sp"
            android:textColor="#ff000000"
            android:gravity="right"
            android:id="@id/tv_moc_review_ach_perc"
            android:padding="4.0dip"
            android:layout_width="60.0dip"
            android:layout_height="wrap_content"
            android:text="%Achy"
            android:layout_weight="1.0" />
    </LinearLayout>
</LinearLayout>