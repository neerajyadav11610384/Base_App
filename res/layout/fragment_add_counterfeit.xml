<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:orientation="vertical"
    android:id="@id/outlist_sheet"
    android:background="#ffffffff"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <RelativeLayout
        android:id="@id/rl"
        android:background="#ffffffff"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
        <androidx.appcompat.widget.Toolbar
            android:id="@id/toolbar"
            android:background="@color/color_2392ff"
            android:fitsSystemWindows="true"
            android:layout_width="fill_parent"
            android:layout_height="?actionBarSize"
            app:contentInsetLeft="0.0dip"
            app:contentInsetStart="0.0dip"
            app:contentInsetStartWithNavigation="0.0dip"
            app:theme="@style/AppTheme.AppBarOverlay">
            <RelativeLayout
                android:orientation="horizontal"
                android:id="@id/title_lyt"
                android:visibility="visible"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <ImageView
                    android:id="@id/img_back_arrow"
                    android:layout_width="@dimen/dimen_24"
                    android:layout_height="@dimen/dimen_24"
                    android:src="@drawable/back_arrow"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dimen_16"
                    android:layout_marginEnd="@dimen/dimen_16"
                    android:layout_alignParentStart="true" />
                <TextView
                    android:textSize="18.0sp"
                    android:textStyle="bold"
                    android:textColor="@color/white"
                    android:ellipsize="end"
                    android:gravity="center|left"
                    android:id="@id/title"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="Counterfeit Products"
                    android:maxLines="1"
                    android:layout_toRightOf="@id/img_back_arrow"
                    android:layout_centerVertical="true" />
            </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView
            android:scrollbars="none"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@id/toolbar">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dimen_16">
                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@id/linearLayout5"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/dimen_8">
                    <TextView
                        android:textSize="@dimen/dimen_16sp"
                        android:textStyle="bold"
                        android:gravity="start|center"
                        android:id="@id/txt_header"
                        android:layout_width="0.0dip"
                        android:layout_height="@dimen/dimen_50"
                        android:text="Outlet Type"
                        android:layout_weight="1.0"
                        android:fontFamily="@font/sf_pro_display_bold"
                        android:textAlignment="viewStart" />
                    <RadioGroup
                        android:orientation="horizontal"
                        android:id="@id/rg_counterfiet"
                        android:layout_width="wrap_content"
                        android:layout_height="50.0dip">
                        <RadioButton
                            android:id="@id/rb_registered"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:checked="true"
                            android:text="Registered"
                            android:layout_marginEnd="@dimen/dimen_4" />
                        <RadioButton
                            android:id="@id/rb_others"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Others"
                            android:layout_marginStart="@dimen/dimen_4" />
                    </RadioGroup>
                </LinearLayout>
                <TextView
                    android:textSize="@dimen/dimen_16sp"
                    android:textStyle="bold"
                    android:gravity="start|center"
                    android:id="@id/tv_choose_outlet"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_16"
                    android:text="Choose Outlet"
                    android:fontFamily="@font/sf_pro_display_bold"
                    android:textAlignment="viewStart" />
                <RelativeLayout
                    android:orientation="horizontal"
                    android:id="@id/rlyt_outlet_spinner"
                    android:background="@drawable/edit_text_white_box_bg"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_8">
                    <Spinner
                        android:layout_gravity="center"
                        android:id="@id/actv_outlet_name"
                        android:background="@android:color/transparent"
                        android:visibility="visible"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:spinnerMode="dropdown"
                        android:overlapAnchor="false" />
                    <ImageView
                        android:layout_gravity="center"
                        android:layout_width="@dimen/dimen_24"
                        android:layout_height="@dimen/dimen_24"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:srcCompat="@drawable/ic_down_arrow" />
                </RelativeLayout>
                <RelativeLayout
                    android:orientation="horizontal"
                    android:id="@id/rlyt_outlet_edit"
                    android:background="@drawable/edit_text_white_box_bg"
                    android:visibility="gone"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_8">
                    <EditText
                        android:textSize="@dimen/dimen_16sp"
                        android:textColor="@color/text_color"
                        android:id="@id/edt_actv_outlet_name"
                        android:background="@null"
                        android:paddingLeft="4.0dip"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:maxLength="200"
                        android:digits="@string/outlet_name_validation"
                        android:imeOptions="actionNext"
                        android:fontFamily="@font/sf_pro_display_regular"
                        android:layout_marginStart="3.0dip" />
                </RelativeLayout>
                <TextView
                    android:textSize="@dimen/dimen_16sp"
                    android:textStyle="bold"
                    android:gravity="start|center"
                    android:id="@id/tv_outlet_image"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_16"
                    android:text="Outlet Image"
                    android:fontFamily="@font/sf_pro_display_bold"
                    android:textAlignment="viewStart" />
                <LinearLayout
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    android:layout_width="196.0dip"
                    android:layout_height="196.0dip"
                    android:layout_marginRight="@dimen/dimen_8"
                    android:layout_weight="1.0">
                    <ImageView
                        android:layout_gravity="right"
                        android:id="@id/iv_outlet_image_btn"
                        android:layout_width="@dimen/dimen_32"
                        android:layout_height="@dimen/dimen_32"
                        android:src="@drawable/ic_camera" />
                    <ImageView
                        android:id="@id/iv_outlet_image"
                        android:background="@drawable/image_border"
                        android:padding="@dimen/dimen_1"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_margin="@dimen/dimen_8"
                        android:src="@drawable/default_img"
                        android:scaleType="fitXY"
                        android:minHeight="170.0dip" />
                </LinearLayout>
                <TextView
                    android:textSize="@dimen/dimen_16sp"
                    android:textStyle="bold"
                    android:gravity="start|center"
                    android:id="@id/tv_branch_name"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_16"
                    android:fontFamily="@font/sf_pro_display_bold"
                    android:textAlignment="viewStart" />
                <com.google.android.material.textfield.TextInputLayout
                    android:enabled="true"
                    android:id="@id/branch_name_text_input"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    app:boxStrokeColor="#ffb4c8dc"
                    app:boxStrokeWidth="@dimen/dimen_1"
                    app:errorEnabled="true"
                    app:errorTextAppearance="@style/ErrorText"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
                    <EditText
                        android:textSize="@dimen/dimen_16sp"
                        android:textColor="@color/text_color"
                        android:id="@id/branch_name"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:maxLength="50"
                        android:lineSpacingExtra="@dimen/dimen_7sp"
                        android:imeOptions="actionNext"
                        android:fontFamily="@font/sf_pro_display_regular" />
                </com.google.android.material.textfield.TextInputLayout>
                <TextView
                    android:textSize="@dimen/dimen_16sp"
                    android:textStyle="bold"
                    android:gravity="start|center"
                    android:id="@id/tv_address_name"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_16"
                    android:fontFamily="@font/sf_pro_display_bold"
                    android:textAlignment="viewStart" />
                <com.google.android.material.textfield.TextInputLayout
                    android:enabled="false"
                    android:id="@id/address_text_input"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    app:boxStrokeColor="#ffb4c8dc"
                    app:boxStrokeWidth="@dimen/dimen_1"
                    app:errorEnabled="true"
                    app:errorTextAppearance="@style/ErrorText"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
                    <EditText
                        android:textSize="@dimen/dimen_16sp"
                        android:textColor="@color/text_color"
                        android:id="@id/address"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="2"
                        android:maxLength="50"
                        android:digits="@string/allowed_only_alphanumaric_custom"
                        android:lineSpacingExtra="@dimen/dimen_2sp"
                        android:imeOptions="actionNext"
                        android:fontFamily="@font/sf_pro_display_regular" />
                </com.google.android.material.textfield.TextInputLayout>
                <TextView
                    android:textSize="@dimen/dimen_16sp"
                    android:textStyle="bold"
                    android:gravity="start|center"
                    android:id="@id/tv_product"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Choose Product"
                    android:fontFamily="@font/sf_pro_display_bold"
                    android:textAlignment="viewStart" />
                <com.google.android.material.textfield.TextInputLayout
                    android:enabled="true"
                    android:id="@id/product_text_input"
                    android:visibility="gone"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    app:boxStrokeColor="#ffb4c8dc"
                    app:boxStrokeWidth="@dimen/dimen_1"
                    app:errorEnabled="true"
                    app:errorTextAppearance="@style/ErrorText"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
                    <EditText
                        android:textSize="@dimen/dimen_16sp"
                        android:textColor="@color/text_color"
                        android:id="@id/edit_product"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="2"
                        android:maxLength="200"
                        android:digits="@string/outlet_name_validation"
                        android:lineSpacingExtra="@dimen/dimen_7sp"
                        android:imeOptions="actionNext"
                        android:fontFamily="@font/sf_pro_display_regular" />
                </com.google.android.material.textfield.TextInputLayout>
                <RelativeLayout
                    android:orientation="horizontal"
                    android:id="@id/rlyt_edt_product_spinner"
                    android:background="@drawable/edit_text_white_box_bg"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_8">
                    <Spinner
                        android:layout_gravity="center"
                        android:id="@id/sp_product_desc"
                        android:background="@android:color/transparent"
                        android:visibility="visible"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:spinnerMode="dropdown"
                        android:overlapAnchor="false" />
                    <ImageView
                        android:layout_gravity="center"
                        android:layout_width="@dimen/dimen_24"
                        android:layout_height="@dimen/dimen_24"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:srcCompat="@drawable/ic_down_arrow" />
                </RelativeLayout>
                <TextView
                    android:textSize="@dimen/dimen_16sp"
                    android:textStyle="bold"
                    android:gravity="start|center"
                    android:id="@id/tv_comments_name"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_16"
                    android:text="Comments"
                    android:fontFamily="@font/sf_pro_display_bold"
                    android:textAlignment="viewStart" />
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@id/comments_text_input"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_12sp"
                    app:boxStrokeColor="#ffb4c8dc"
                    app:boxStrokeWidth="@dimen/dimen_1"
                    app:errorEnabled="true"
                    app:errorTextAppearance="@style/ErrorText"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
                    <EditText
                        android:textSize="@dimen/dimen_16sp"
                        android:textColor="@color/text_color"
                        android:id="@id/comments"
                        android:layout_width="fill_parent"
                        android:layout_height="100.0dip"
                        android:hint="@string/label_comments"
                        android:maxLines="3"
                        android:maxLength="200"
                        android:imeOptions="actionNext"
                        android:fontFamily="@font/sf_pro_display_regular" />
                </com.google.android.material.textfield.TextInputLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_8"
                        android:layout_weight="1.0">
                        <ImageView
                            android:layout_gravity="right"
                            android:id="@id/iv_top_image_btn"
                            android:padding="@dimen/dimen_4"
                            android:layout_width="@dimen/dimen_32"
                            android:layout_height="@dimen/dimen_32"
                            android:layout_marginRight="@dimen/dimen_4"
                            android:src="@drawable/ic_camera" />
                        <ImageView
                            android:id="@id/iv_top_image"
                            android:background="@drawable/image_border"
                            android:padding="@dimen/dimen_1"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/dimen_100"
                            android:src="@drawable/default_img"
                            android:scaleType="fitXY" />
                        <TextView
                            android:gravity="center"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Top" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_6"
                        android:layout_marginRight="@dimen/dimen_6"
                        android:layout_weight="1.0">
                        <ImageView
                            android:layout_gravity="right"
                            android:id="@id/iv_front_image_btn"
                            android:padding="@dimen/dimen_4"
                            android:layout_width="@dimen/dimen_32"
                            android:layout_height="@dimen/dimen_32"
                            android:layout_marginRight="@dimen/dimen_4"
                            android:src="@drawable/ic_camera" />
                        <ImageView
                            android:id="@id/iv_front_image"
                            android:background="@drawable/image_border"
                            android:padding="@dimen/dimen_1"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/dimen_100"
                            android:src="@drawable/default_img"
                            android:scaleType="fitXY" />
                        <TextView
                            android:gravity="center"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Front" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_8"
                        android:layout_weight="1.0">
                        <ImageView
                            android:layout_gravity="right"
                            android:id="@id/iv_left_image_btn"
                            android:padding="@dimen/dimen_4"
                            android:layout_width="@dimen/dimen_32"
                            android:layout_height="@dimen/dimen_32"
                            android:layout_marginRight="@dimen/dimen_4"
                            android:src="@drawable/ic_camera" />
                        <ImageView
                            android:id="@id/iv_left_image"
                            android:background="@drawable/image_border"
                            android:padding="@dimen/dimen_1"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/dimen_100"
                            android:src="@drawable/default_img"
                            android:scaleType="fitXY" />
                        <TextView
                            android:gravity="center"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Left" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_10">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_8"
                        android:layout_weight="1.0">
                        <ImageView
                            android:layout_gravity="right"
                            android:id="@id/iv_bottom_image_btn"
                            android:padding="@dimen/dimen_4"
                            android:layout_width="@dimen/dimen_32"
                            android:layout_height="@dimen/dimen_32"
                            android:layout_marginRight="@dimen/dimen_4"
                            android:src="@drawable/ic_camera" />
                        <ImageView
                            android:id="@id/iv_bottom_image"
                            android:background="@drawable/image_border"
                            android:padding="@dimen/dimen_1"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/dimen_100"
                            android:src="@drawable/default_img"
                            android:scaleType="fitXY" />
                        <TextView
                            android:gravity="center"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Bottom" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_6"
                        android:layout_marginRight="@dimen/dimen_6"
                        android:layout_weight="1.0">
                        <ImageView
                            android:layout_gravity="right"
                            android:id="@id/iv_back_image_btn"
                            android:padding="@dimen/dimen_4"
                            android:layout_width="@dimen/dimen_32"
                            android:layout_height="@dimen/dimen_32"
                            android:layout_marginRight="@dimen/dimen_4"
                            android:src="@drawable/ic_camera" />
                        <ImageView
                            android:id="@id/iv_back_image"
                            android:background="@drawable/image_border"
                            android:padding="@dimen/dimen_1"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/dimen_100"
                            android:src="@drawable/default_img"
                            android:scaleType="fitXY" />
                        <TextView
                            android:gravity="center"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Back" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_8"
                        android:layout_weight="1.0">
                        <ImageView
                            android:layout_gravity="right"
                            android:id="@id/iv_right_image_btn"
                            android:padding="@dimen/dimen_4"
                            android:layout_width="@dimen/dimen_32"
                            android:layout_height="@dimen/dimen_32"
                            android:layout_marginRight="@dimen/dimen_4"
                            android:src="@drawable/ic_camera" />
                        <ImageView
                            android:id="@id/iv_right_image"
                            android:background="@drawable/image_border"
                            android:padding="@dimen/dimen_1"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/dimen_100"
                            android:src="@drawable/default_img"
                            android:scaleType="fitXY" />
                        <TextView
                            android:gravity="center"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Right" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dimen_50"
                    android:layout_marginTop="@dimen/dimen_32"
                    android:layout_marginBottom="@dimen/dimen_16">
                    <androidx.appcompat.widget.AppCompatTextView
                        android:textSize="@dimen/dimen_16sp"
                        android:textColor="#ff2392ff"
                        android:gravity="center"
                        android:id="@id/tv_cancel"
                        android:background="@drawable/bg_view_order"
                        android:visibility="visible"
                        android:layout_width="0.0dip"
                        android:layout_height="fill_parent"
                        android:text="@string/cancel"
                        android:layout_weight="1.0"
                        android:lineSpacingExtra="@dimen/dimen_7sp"
                        android:fontFamily="@font/sf_pro_display_bold"
                        android:layout_marginEnd="@dimen/dimen_10" />
                    <androidx.appcompat.widget.AppCompatTextView
                        android:textSize="@dimen/dimen_18sp"
                        android:textStyle="bold"
                        android:textColor="@color/white"
                        android:gravity="center"
                        android:id="@id/tv_done"
                        android:background="@drawable/bg_pending_amount"
                        android:visibility="visible"
                        android:layout_width="0.0dip"
                        android:layout_height="fill_parent"
                        android:text="@string/submit"
                        android:layout_weight="1.0"
                        android:lineSpacingExtra="@dimen/dimen_7sp"
                        android:textAllCaps="false"
                        android:fontFamily="@font/sf_pro_display_bold"
                        android:layout_marginStart="@dimen/dimen_10" />
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>
    <include
        layout="@layout/activity_custom_camera_new" />
</RelativeLayout>