<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    android:background="#ffffffff"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:gravity="top"
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:id="@id/ll_mocd_parent"
                android:background="#ffebf5ff"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:background="@drawable/background_moc_dashboard_background_white_border_color_grey"
                    android:layout_width="wrap_content"
                    android:layout_height="40.0dip">
                    <ImageView
                        android:layout_gravity="center"
                        android:id="@id/img_circle_plus_minus"
                        android:visibility="invisible"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/circle_plus" />
                    <TextView
                        android:textSize="12.0sp"
                        android:textStyle="normal"
                        android:textColor="#ff000000"
                        android:gravity="center|left"
                        android:id="@id/tv_mocdc_key_name"
                        android:padding="4.0dip"
                        android:layout_width="110.0dip"
                        android:layout_height="wrap_content"
                        android:text="Secondary Sales"
                        android:layout_weight="3.0" />
                </LinearLayout>
                <TextView
                    android:textSize="12.0sp"
                    android:textStyle="normal"
                    android:textColor="#ff000000"
                    android:gravity="center"
                    android:id="@id/tv_mocdc_targets"
                    android:background="@drawable/background_moc_dashboard_background_white_border_color_grey"
                    android:padding="4.0dip"
                    android:layout_width="70.0dip"
                    android:layout_height="40.0dip"
                    android:text="3101183"
                    android:layout_weight="1.0" />
                <TextView
                    android:textSize="12.0sp"
                    android:textStyle="normal"
                    android:textColor="#ff000000"
                    android:gravity="center"
                    android:id="@id/tv_mocdc_achivement"
                    android:background="@drawable/background_moc_dashboard_background_white_border_color_grey"
                    android:padding="4.0dip"
                    android:layout_width="70.0dip"
                    android:layout_height="40.0dip"
                    android:text="3101183"
                    android:layout_weight="1.0" />
                <TextView
                    android:textSize="12.0sp"
                    android:textStyle="normal"
                    android:textColor="#ff000000"
                    android:gravity="center"
                    android:id="@id/tv_mocdc_perc_calc"
                    android:background="@drawable/background_moc_dashboard_background_white_border_color_grey"
                    android:padding="4.0dip"
                    android:layout_width="40.0dip"
                    android:layout_height="40.0dip"
                    android:text="104"
                    android:layout_weight="1.0" />
                <TextView
                    android:textSize="12.0sp"
                    android:textStyle="normal"
                    android:textColor="#ff000000"
                    android:gravity="center"
                    android:id="@id/tv_mocdc_seontrack"
                    android:background="@drawable/background_moc_dashboard_background_white_border_color_grey"
                    android:padding="4.0dip"
                    android:layout_width="40.0dip"
                    android:layout_height="40.0dip"
                    android:text="0"
                    android:layout_weight="1.0" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>