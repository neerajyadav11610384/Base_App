<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    android:layout_gravity="bottom"
    android:id="@id/actions_container"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:orientation="horizontal"
        android:id="@id/actions"
        android:padding="3.0dip"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/we_push_action_list_height">
        <TextView
            android:textSize="14.0sp"
            android:textColor="#ff000000"
            android:ellipsize="end"
            android:gravity="center|left"
            android:id="@id/action1_native"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:maxWidth="95.0dip"
            android:text="Action-1"
            android:maxLines="1"
            android:fontFamily="sans-serif"
            android:paddingEnd="@dimen/we_push_action_padding" />
        <TextView
            android:textSize="14.0sp"
            android:textColor="#ff000000"
            android:ellipsize="end"
            android:gravity="center|left"
            android:id="@id/action2_native"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_marginLeft="4.0dip"
            android:maxWidth="95.0dip"
            android:text="Action-2"
            android:maxLines="1"
            android:fontFamily="sans-serif"
            android:paddingStart="@dimen/we_push_action_padding"
            android:paddingEnd="@dimen/we_push_action_padding" />
        <TextView
            android:textSize="14.0sp"
            android:textColor="#ff000000"
            android:ellipsize="end"
            android:gravity="center|left"
            android:id="@id/action3_native"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_marginLeft="4.0dip"
            android:maxWidth="95.0dip"
            android:text="Action-3"
            android:maxLines="1"
            android:fontFamily="sans-serif"
            android:paddingStart="@dimen/we_push_action_padding"
            android:paddingEnd="@dimen/we_push_action_padding" />
    </LinearLayout>
</FrameLayout>